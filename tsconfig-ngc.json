{
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es2015",

    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "./dist-ngc",
    "lib": ["es2015", "dom"]
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./dist-ts/select-dict.module.ts"
  ],
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": false
  }
}
